-- ==================== TAB 3 - √ÇM THANH ====================
local sound = Instance.new("Sound", workspace)
sound.SoundId = "rbxassetid://966101142098" -- thay ID √¢m thanh n·∫øu mu·ªën
sound.Volume = 2
sound.Looped = true
local playing = false

AddButton(Tab3o, {
    Name = "üîä B·∫≠t/T·∫Øt Nh·∫°c",
    Callback = function()
        playing = not playing
        if playing then
            sound:Play()
            game.StarterGui:SetCore("SendNotification", {Title="√Çm thanh", Text="üéµ ƒêang ph√°t nh·∫°c", Duration=2})
        else
            sound:Stop()
            game.StarterGui:SetCore("SendNotification", {Title="√Çm thanh", Text="üîá ƒê√£ t·∫Øt nh·∫°c", Duration=2})
        end
    end
})
