local success, err = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/daucobonhi/Ui-Redz-V2/refs/heads/main/UiREDzV2.lua"))()
end)

if not success then
    warn("Kh√¥ng load ƒë∆∞·ª£c UI Library: " .. tostring(err))
    return
end

-- T·∫°o Window (t·∫°m th·ªùi t·∫°o, nh∆∞ng n·ªôi dung s·∫Ω ch·∫∑n cho ƒë·∫øn khi x√°c th·ª±c)
